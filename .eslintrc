{
  "extends": ["plugin:tailwindcss/recommended"],
  "rules": {
    "no-console": ["error", { "allow": ["info", "warn", "error"] }],
    "tailwindcss/classnames-order": "error"
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js"],
      "parser": "@typescript-eslint/parser"
    }
  ]
}
